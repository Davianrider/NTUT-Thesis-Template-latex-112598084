@PhdThesis{112TIT00392032,
author={{温淇淼}},
editor={{吳和庭}
and WU, HO-TING},
title={基於FruityMesh之藍牙低功耗網狀網路傳輸機制的設計以改善QoS},
series={資訊工程系},
year={2024},
publisher={國立臺北科技大學},
address={台北市},
volume={碩士},
pages={41},
keywords={藍牙低功耗; BLE Mesh; FruityMesh; 分時多工; QoS; 傳輸機制; 重傳; Bluetooth Low Energy; TDMA; Transmission; Mechanism; Retransmission},
abstract={本論文探討於BLE Mesh網路中實現TDMA、即時通訊方法，time slot的範圍的設計。在配置BLE Mesh中合適的Time Slot以實現TDMA時，須先解決因BLE Mesh節點可能在同一時間具有多重角色身份而引起的重傳、衝突等問題。本研究基於開源軟體Fruitymesh上設計並實現DOST(Destination-Oriented Switchable Transmission)的傳輸機制。DOST機制通過啟用/禁用連線的方法來間隔BLE Mesh樹狀架構中節點同時傳輸之情況，從而有效避免通訊重疊和節點角色身分衝突，藉此降低封包重傳的可能性。在Fruitymesh上，本研究之傳輸機制DOST主要包括兩個功能：一、利用樹狀結構中level的特性導入各連線與節點中，並新增傳輸規則，使傳輸具有目的導向、方向性，減少網路上不必要的傳輸，從而降低網路負載。二、DOST同樣利用level的特性實現連線的啟用與禁用，藉由level區分節點的不同連線，以此劃分連線的啟用或禁用狀態。透過於真實場景下實驗，結果顯示本研究之傳輸機制DOST能有效的提升效率，並減輕網路的負載。},
url={https://hdl.handle.net/11296/7u74xv}
}


